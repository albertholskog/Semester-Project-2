{
   "name": "semester-project-2",
   "version": "1.0.0",
   "description": "semester project",
   "main": "index.js",
   "scripts": {
      "build": "sass scss:css",
      "watch": "sass --watch sass scss:css",
      "test": "npm run test-unit",
      "test-unit": "jest",
      "prepare": "husky install",
      "test-e2e": "cypress open",
      "test-e2e-cli": "cypress run",
      "docs": "jsdoc -c ./conf.json -r"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/albertholskog/Semester-Project-2.git"
   },
   "author": "albert holskog",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/albertholskog/Semester-Project-2/issues"
   },
   "homepage": "https://github.com/albertholskog/Semester-Project-2#readme",
   "devDependencies": {
      "@babel/core": "^7.19.3",
      "@babel/preset-env": "^7.19.4",
      "bootstrap": "^5.2.0",
      "cypress": "^10.7.0",
      "eslint": "^8.27.0",
      "eslint-plugin-cypress": "^2.12.1",
      "eslint-plugin-jest": "^27.1.5",
      "husky": "^8.0.2",
      "jest": "^29.3.1",
      "jsdoc": "^4.0.0",
      "lint-staged": "^13.0.3",
      "prettier": "^2.7.1",
      "sass": "^1.56.1"
   },
   "lint-staged": {
      "*.js": [
         "eslint --fix",
         "prettier --write"
      ],
      "*.mjs": [
         "eslint --fix",
         "prettier --write"
      ],
      "*.html": [
         "prettier --write"
      ],
      "*.scss": [
         "prettier --write"
      ]
   },
   "dependencies": {
      "bootstrap-icons": "^1.10.2"
   }
}
